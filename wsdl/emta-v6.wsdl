                    <element name="target" type="string">
                        <annotation>
                            <appinfo>
                                <xrd:title>Teenuse nimi</xrd:title>
                                <xrd:notes>Maksu- ja Tolliameti süsteemi (teenus) lühinimi, näiteks TSD, KMD</xrd:notes>
                            </appinfo>
