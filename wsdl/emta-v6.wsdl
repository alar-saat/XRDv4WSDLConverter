                        </sequence>
                        <sequence>
                            <element minOccurs="0" name="file">
                                <annotation>
                                    <appinfo>
                                        <xrd:title>Manus</xrd:title>
                                        <xrd:techNotes>Viide manusele (Content-Id)</xrd:techNotes>
                                    </appinfo>
                                </annotation>
                                <complexType>
                                    <attribute name="href" type="string"/>
                                </complexType>
                            </element>
                            <element name="props">
                                <annotation>
                                    <appinfo>
                                        <xrd:title>Manuse metaandmed</xrd:title>
                                        <xrd:notes>Manust kirjeldavad parameetrid</xrd:notes>
                                    </appinfo>
                                </annotation>
                                <complexType>
                                    <sequence>
                                        <element maxOccurs="unbounded" minOccurs="0" name="prop">
                                            <annotation>
                                                <documentation>Parameetri väärtus</documentation>
                                            </annotation>
                                            <complexType>
                                                <simpleContent>
                                                    <extension base="string">
